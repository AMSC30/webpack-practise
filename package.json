        "serve:dev": "webpack serve --config ./build/webpack.dev.js",
        "serve:prod": "webpack serve --config ./build/webpack.prod.js",
        "lib": "webpack --config ./src/library/config.js",
        "shaking": "webpack --config ./src/shaking/config.js",
        "loader": "webpack --config ./src/loader/config.js"
